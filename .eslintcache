difiedItem) => {\n    this.props.history.push(`/edit/${modifiedItem.id}`)\n  }\n  deleteItem = (deletedItem) => {\n    this.props.actions.deleteItem(deletedItem)\n  }\n\n  render() {\n    const { data } = this.props\n    const { items, categories, currentDate } = data\n    const { tabView } = this.state\n    // 把items和category给链接起来 生成新的数据\n    const itemsWithCategory = Object.keys(items).map((id) => {\n      items[id].category = categories[items[id].cid]\n      return items[id]\n    })\n\n    let totalIncome = 0,\n      totalOutcome = 0\n    itemsWithCategory.forEach((item) => {\n      if (item.category.type === TYPE_OUTCOME) {\n        totalOutcome += item.price\n      } else {\n        totalIncome += item.price\n      }\n    })\n    return (\n      <React.Fragment>\n        <header className=\"App-header\">\n          <div className=\"row mb-5 justify-content-center\">\n            <img src={logo} className=\"App-logo\" alt=\"logo\" />\n          </div>\n          <div className=\"row\">\n            <div className=\"col\">\n              <MonthPicker\n                year={currentDate.year}\n                month={currentDate.month}\n                onChange={this.changeDate}\n              />\n            </div>\n            <div className=\"col\">\n              <TotalPrice income={totalIncome} outcome={totalOutcome} />\n            </div>\n          </div>\n        </header>\n        <div className=\"content-area py-3 px-3\">\n          <Tabs activeIndex={0} onTabChange={this.changeView}>\n            <Tab>\n              <Ionicon\n                className=\"rounded-circle mr-2\"\n                fontSize=\"25px\"\n                color={'#007bff'}\n                icon={'ios-paper'}\n              />\n              列表模式\n            </Tab>\n            <Tab>\n              <Ionicon\n                className=\"rounded-circle mr-2\"\n                fontSize=\"25px\"\n                color={'#007bff'}\n                icon={'ios-pie'}\n              />\n              图表模式\n            </Tab>\n          </Tabs>\n          <CreateBtn onClick={this.createItem} />\n          {tabView === LIST_VIEW && (\n            <PriceList\n              items={itemsWithCategory}\n              onModifyItem={this.modifyItem}\n              onDeleteItem={this.deleteItem}\n            />\n          )}\n          {tabView === CHART_VIEW && (\n            <h1 className=\"chart-title\">这里是图表区域</h1>\n          )}\n        </div>\n      </React.Fragment>\n    )\n  }\n}\n\nexport default withRouter(withContext(Home))\n","/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/containers/Create.js",[],"/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/components/CategorySelect.js",[],"/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/components/PriceForm.js",[],"/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/components/Tabs.js",[],"/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/testData.js",[],"/Users/tsai/Desktop/项目/练习的项目/React/online_books/src/WithContext.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"70","replacedBy":"74"},{"ruleId":"72","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":8,"column":3,"nodeType":"78","messageId":"79","endLine":8,"endColumn":22},"no-native-reassign",["80"],"no-negated-in-lhs",["81"],["80"],["81"],"no-unused-vars","'parseToYearAndMonth' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]